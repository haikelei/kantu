<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.PicDetailEntity" table="pic_detail" schema="xianliao">
        <id name="index" column="index">
            <generator class="identity" />
        </id>
        <property name="modelId" column="model_id"/>
        <property name="atlasId" column="atlas_id"/>
        <property name="url" column="url"/>
        <property name="indexInAtlas" column="index_in_atlas"/>
    </class>
</hibernate-mapping>