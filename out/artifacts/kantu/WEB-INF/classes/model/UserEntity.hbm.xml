<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.UserEntity" table="user" schema="xianliao">
        <id name="uid" column="uid">
            <generator class="identity"></generator>
        </id>
        <property name="vipBuytime" column="vip_buytime"/>
        <property name="vipDeadline" column="vip_deadline"/>
        <property name="nickName" column="nick_name"/>
        <property name="avatar" column="avatar"/>
        <property name="deviceId" column="deviceId"/>
    </class>
</hibernate-mapping>